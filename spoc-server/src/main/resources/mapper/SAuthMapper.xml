<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsy.server.mapper.SAuthMapper">

    <resultMap id="BaseResultMap" type="com.wsy.server.domain.SAuth">
            <id property="authId" column="auth_id" jdbcType="INTEGER"/>
            <result property="authName" column="auth_name" jdbcType="VARCHAR"/>
            <result property="authDesc" column="auth_desc" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        auth_id,auth_name,auth_desc
    </sql>
</mapper>
