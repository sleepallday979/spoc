<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.SQuestionMapper">

    <resultMap id="BaseResultMap" type="com.wsy.server.domain.SQuestion">
            <id property="questionId" column="question_id" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="courseId" column="course_id" jdbcType="INTEGER"/>
            <result property="questionName" column="question_name" jdbcType="VARCHAR"/>
            <result property="questionDesc" column="question_desc" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        question_id,user_id,course_id,
        question_name,question_desc
    </sql>
</mapper>
